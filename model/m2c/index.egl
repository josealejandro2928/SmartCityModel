            <div class="col-sm-6 col-md-4">
              <div class="card" data-toggle="tooltip" data-placement="top" title="Tooltip on top">
                <div class="card-body">
                  <h4 class="card-title">[%=kpi.averageBandWidth.name%] KPI</h4>
                  <p class="card-description my-0 mb-3"  
                    title="Shows the ratio between the bandwidth average on the city and an excellent internet speed 100 Mbps">

                    Shows the ratio between the bandwidth average on the city and an excellent
                    internet speed
                    <strong>100 Mbits/sec.</strong>
                  </p>

                  <div class="my-labels">
                    <div class="btn-group" role="group" aria-label="Basic mixed styles example">
                    
                    [% for (c in kpi.averageBandWidth.labels) { %]
                        [%  var type = "btn-success";
                        	var disabled = "disabled";
                    	    if (c.name == "Regular") {
                    	    	type = "btn-warning";
                    		}
                    		if (c.name == "Bad") {
                    	    	type = "btn-danger";
                    	    	disabled = "";
                    		}
                    		if(c.name == kpi.averageBandWidth.labelValue){
                    			type += " activated";
                    		}
                    	%]
						 <button type="button" class="btn [%=type%]" [%=disabled%]>[%=c.name%]</button>
					[% } %]   
                    </div>
                  </div>
                  [% 
                  		var typeBg = "bg-success";
                  		var typeText = "text-success";
                        if (kpi.averageBandWidth.labelValue == "Regular") {
                    	    typeBg = "bg-warning";
                    	    typeText = "text-warning";
                    	}
                    	if (kpi.averageBandWidth.labelValue == "Bad") {
                    	    typeBg = "bg-danger";
                    	    typeText = "text-danger";	
                    	}
                  %]
                  <span class="[%=typeText%]">[%=kpi.averageBandWidth.quantitiveValue + "%"%]</span>
                  <div class="progress">
                    <div
                      class="progress-bar [%=typeBg%]"
                      role="progressbar"
                      style="width: [%=kpi.averageBandWidth.quantitiveValue + "%"%]; height: 6px"
                      aria-valuenow="25"
                      aria-valuemin="0"
                      aria-valuemax="100"
                    ></div>
                  </div>
                </div>
              </div>
            </div>
            
