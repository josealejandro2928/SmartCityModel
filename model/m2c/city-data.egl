                      id="navbarDropdown"
                      role="button"
                      data-bs-toggle="dropdown"
                      aria-expanded="false"
                    >
                      <img
                        src="./assets/images/users/image-perfil.png"
                        alt="user"
                        class="profile-pic me-2"
                      />Jose Alejandro
                    </a>
                    <ul class="dropdown-menu show" aria-labelledby="navbarDropdown"></ul>
                  </li>
                </ul>
              </div>
            </nav>
          </header>
        <!-- ============================================================== -->
        <!-- End Topbar header -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Left Sidebar - style you can find in sidebar.scss  -->
        <!-- ============================================================== -->
        <aside class="left-sidebar" data-sidebarbg="skin6">
            <!-- Sidebar scroll-->
            <div class="scroll-sidebar">
                <!-- Sidebar navigation-->
                <nav class="sidebar-nav">
                    <ul id="sidebarnav">
                        <!-- User Profile-->
                        <li class="sidebar-item">
                          <a
                            class="sidebar-link waves-effect waves-dark sidebar-link"
                            href="index.html"
                            aria-expanded="false"
                            ><i class="me-3 far fa-clock fa-fw" aria-hidden="true"></i
                            ><span class="hide-menu">Dashboard</span></a
                          >
                        </li>
                        <li class="sidebar-item">
                          <a
                            class="sidebar-link waves-effect waves-dark sidebar-link"
                            href="city-data.html"
                            aria-expanded="false"
                            ><i class="me-3 fa fa-table" aria-hidden="true"></i
                            ><span class="hide-menu">City Data</span></a
                          >
                        </li>

                        <li class="sidebar-item">
                          <a
                            class="sidebar-link waves-effect waves-dark sidebar-link"
                            href="pages-profile.html"
                            aria-expanded="false"
                          >
                            <i class="me-3 fa fa-user" aria-hidden="true"></i
                            ><span class="hide-menu">Authors</span></a
                          >
                        </li>
                       
                        <li class="sidebar-item">
                          <a
                            class="sidebar-link waves-effect waves-dark sidebar-link"
                            href="map-google.html"
                            aria-expanded="false"
                            ><i class="me-3 fa fa-globe" aria-hidden="true"></i
                            ><span class="hide-menu">City Map</span></a
                          >
                        </li>
                      </ul>
                </nav>
                <!-- End Sidebar navigation -->
            </div>
            <!-- End Sidebar scroll-->
        </aside>
        <!-- ============================================================== -->
        <!-- End Left Sidebar - style you can find in sidebar.scss  -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Page wrapper  -->
        <!-- ============================================================== -->
        <div class="page-wrapper">
            <!-- ============================================================== -->
            <!-- Bread crumb and right sidebar toggle -->
            <!-- ============================================================== -->
            <div class="page-breadcrumb">
                <div class="row align-items-center">
                    <div class="col-md-6 col-8 align-self-center">
                        <h3 class="page-title mb-0 p-0">Tables</h3>
                        <div class="d-flex align-items-center">
                            <nav aria-label="breadcrumb">
                                <ol class="breadcrumb">
                                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                                    <li class="breadcrumb-item active" aria-current="page">City Data</li>
                                </ol>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>
            <!-- ============================================================== -->
            <!-- End Bread crumb and right sidebar toggle -->
            <!-- ============================================================== -->
            <!-- ============================================================== -->
            <!-- Container fluid  -->
            <!-- ============================================================== -->
            <div class="container-fluid">
                <!-- ============================================================== -->
                <!-- Start Page Content -->
                <!-- ============================================================== -->
                <div class="row">
                	<!-- ============================================================== -->
                    <!-- City -->
                    <!-- ============================================================== -->
                    <div class="col-12 col-md-12">
                        <div class="card">
                            <div class="card-body">
                                <h4 class="card-title">City</h4>
                                <div class="table-responsive table-mod">
                                    <table class="table user-table no-wrap">
                                        <thead>
                                            <tr>
                                                <th class="border-top-0">name</th>
                                                <th class="border-top-0">population</th>
                                                <th class="border-top-0">area</th>
                                                <th class="border-top-0">location</th>
                                                <th class="border-top-0"># houses</th>
                                                <th class="border-top-0"># services</th>
                                                <th class="border-top-0"># mobiles antenna</th>
                                            </tr>
                                        </thead>
                                        <tbody>
	                                           <tr>
	                                                <td>[%=city.name%]</td>
	                                                <td>[%=city.population%]</td>
	                                                <td>[%=city.area%]</td>
	                                                 <td>
	                                                     <ul>
	                                                     	<li> lat = [%=city.lat%]</li>
	                                                     	<li> lng = [%=city.lng%]</li>
	                                                     </ul>
	                                                 </td>
	                                                <td>[%=city.houses.size()%]</td>
	                                                <td>[%=city.services.size()%]</td>
	                                                <td>[%=city.mobileNetworks.size()%]</td>
                                            	</tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    
                </div>
                <div class="row">
                    <!-- ============================================================== -->
                    <!-- HOUSES -->
                    <!-- ============================================================== -->
                    <div class="col-12 col-md-6">
                        <div class="card">
                            <div class="card-body">
                                <h4 class="card-title">Houses</h4>
                                <div class="table-responsive table-mod">
                                    <table class="table user-table no-wrap">
                                        <thead>
                                            <tr>
                                                <th class="border-top-0">#</th>
                                                <th class="border-top-0">address</th>
                                                <th class="border-top-0">internet</th>
                                                <th class="border-top-0">bandwidth</th>
                                                <th class="border-top-0">smart elec. meter</th>
                                                <th class="border-top-0">energy class</th>
                                                
                                                
                                            </tr>
                                        </thead>
                                        <tbody>
                                            [% var index = 0; %]
	                                        [% for (c in city.houses) { %]
	                                           [% index+=1;%]
	                                           <tr>
	                                                <td>[%= index %]</td>
	                                                <td>[%= c.address %]</td>
	                                                <td>[%= (c.hasInternetAccess) ? "<span class='text-success'>Yes</span>": "<span class='text-danger'>No</span>" %]</td>
	                                                <td>[%= c.bandWidth %] Mbps</td>
	                                                <td>[%= (c.hasSmartElectricMeter) ? "<span class='text-success'>Yes</span>": "<span class='text-danger'>No</span>" %]</td>
	                                                <td>[%= c.electricalClass %]</td>
                                            	</tr>
											[% } %]   
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- ============================================================== -->
                    <!-- SERVICES -->
                    <!-- ============================================================== -->
                    <div class="col-12 col-md-6">
                        <div class="card">
                            <div class="card-body">
                                <h4 class="card-title">Services</h4>
                                <div class="table-responsive table-mod">
                                    <table class="table user-table no-wrap">
                                        <thead>
                                            <tr>
                                                <th class="border-top-0">#</th>
                                                <th class="border-top-0">name</th>
                                                <th class="border-top-0">web or app</th>
                                                <th class="border-top-0">public wifi</th>
                                                <th class="border-top-0">bandwidth</th>
                                                <th class="border-top-0">e-payments</th>
                                                
                                                
                                            </tr>
                                        </thead>
                                        <tbody>
                                            [% var index = 0; %]
	                                        [% for (c in city.services) { %]
	                                           [% index+=1;%]
	                                           <tr>
	                                                <td>[%= index %]</td>
	                                                <td>[%= c.name %]</td>
	                                                <td>[%= (c.hasWebOrMobileApp) ? "<span class='text-success'>Yes</span>": "<span class='text-danger'>No</span>" %]</td>
	                                                <td>[%= (c.hasPublicWiFi) ? "<span class='text-success'>Yes</span>": "<span class='text-danger'>No</span>" %]</td>
	                                                <td>[%= c.bandWidth %] Mbps</td>
	                                                <td>[%= (c.isAvailableEPayments) ? "<span class='text-success'>Yes</span>": "<span class='text-danger'>No</span>" %]</td>
                                            	</tr>
											[% } %]   
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>


                    
                </div>
                <!-- ============================================================== -->
                <!-- End PAge Content -->
                <!-- ============================================================== -->
                <!-- ============================================================== -->
                <!-- Right sidebar -->
                <!-- ============================================================== -->
                <!-- .right-sidebar -->
                <!-- ============================================================== -->
                <!-- End Right sidebar -->
                <!-- ============================================================== -->
            </div>
            <!-- ============================================================== -->
            <!-- End Container fluid  -->
            <!-- ============================================================== -->
            <!-- ============================================================== -->
            <!-- footer -->
            <!-- ============================================================== -->
             <footer class="footer text-center">
              © 2021 MDE Project Admin by
              <a href="https://josealejandro2928.github.io/portfolio/" target="_blank">J.A.</a>
            </footer>
            <!-- ============================================================== -->
            <!-- End footer -->
            <!-- ============================================================== -->
        </div>
        <!-- ============================================================== -->
        <!-- End Page wrapper  -->
        <!-- ============================================================== -->
    </div>
    <!-- ============================================================== -->
    <!-- End Wrapper -->
    <!-- ============================================================== -->
    <!-- ============================================================== -->
    <!-- All Jquery -->
    <!-- ============================================================== -->
    <script src="./assets/plugins/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap tether Core JavaScript -->
    <script src="./assets/plugins/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="js/app-style-switcher.js"></script>
    <!--Wave Effects -->
    <script src="js/waves.js"></script>
    <!--Menu sidebar -->
    <script src="js/sidebarmenu.js"></script>
    <!--Custom JavaScript -->
    <script src="js/custom.js"></script>
</body>

