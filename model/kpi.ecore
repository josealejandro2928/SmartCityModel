<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="kpiMM" nsURI="kpi.project.mde" nsPrefix="kpiMM">
  <eClassifiers xsi:type="ecore:EClass" name="KPI">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="cityName" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="averageBandWidth" eType="#//IndicatorKPI"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="percentHousesAccesToInternet"
        eType="#//IndicatorKPI" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="percentHousesBadEnergyEfficiency"
        eType="#//IndicatorKPI" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="percentHousesSmartElectricMeter"
        eType="#//IndicatorKPI" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="IndicatorKPI">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="quantitiveValue" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EFloat"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="labelValue" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="labels" upperBound="-1"
        eType="#//Label" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Label">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
</ecore:EPackage>
